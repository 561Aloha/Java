// Subclass Helicopter
public class Helicopter extends AircraftSelector {
    @Override
    public void takeoff() {
        System.out.println("Helicopter is taking of vertically.");
    }

    @Override
    public void fly() {
        System.out.println("Helicopter is flying vertically.");
    }

    @Override
    public void land() {
        System.out.println("Helicopter is landing vertically, kapoosh.");
    }
}
